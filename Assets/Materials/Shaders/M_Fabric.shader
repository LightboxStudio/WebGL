				float2 uv068 = IN.ase_texcoord7.xy * temp_cast_0 + float2( 0,0 );
				float2 temp_cast_1 = (_MATERIALTILING_Instance).xx;
				float2 uv169 = IN.ase_texcoord7.zw * temp_cast_1 + float2( 0,0 );
				#if defined(_UV_SET01_UV0)
				float2 staticSwitch67 = uv068;
				#elif defined(_UV_SET01_UV1)
				float2 staticSwitch67 = uv169;
				#else
				float2 staticSwitch67 = uv068;
				#endif
				float _ALBEDOluminance_Instance = UNITY_ACCESS_INSTANCED_PROP(KETTAL_Fabric,_ALBEDOluminance);
				float4 saferPower16 = max( ( tex2D( _ALBEDOtext, staticSwitch67 ) * _ALBEDOluminance_Instance ) , 0.0001 );
				float _ALBEDOcontrast_Instance = UNITY_ACCESS_INSTANCED_PROP(KETTAL_Fabric,_ALBEDOcontrast);
				float4 temp_cast_2 = (_ALBEDOcontrast_Instance).xxxx;
				float _ALBEDOsaturation_Instance = UNITY_ACCESS_INSTANCED_PROP(KETTAL_Fabric,_ALBEDOsaturation);
				float3 desaturateInitialColor18 = pow( saferPower16 , temp_cast_2 ).rgb;
				float desaturateDot18 = dot( desaturateInitialColor18, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar18 = lerp( desaturateInitialColor18, desaturateDot18.xxx, ( 1.0 - _ALBEDOsaturation_Instance ) );
				float _FRESNELsaturation_Instance = UNITY_ACCESS_INSTANCED_PROP(KETTAL_Fabric,_FRESNELsaturation);
				float3 desaturateInitialColor11 = desaturateVar18;
				float desaturateDot11 = dot( desaturateInitialColor11, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar11 = lerp( desaturateInitialColor11, desaturateDot11.xxx, ( 1.0 - _FRESNELsaturation_Instance ) );
				float _FRESNELluminance_Instance = UNITY_ACCESS_INSTANCED_PROP(KETTAL_Fabric,_FRESNELluminance);
