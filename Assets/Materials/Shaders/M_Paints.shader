				float _TILINGtexture_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_TILINGtexture);
				float2 temp_cast_0 = (_TILINGtexture_Instance).xx;
				float2 uv08 = IN.ase_texcoord7.xy * temp_cast_0 + float2( 0,0 );
				float _ALBEDOgain_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_ALBEDOgain);
