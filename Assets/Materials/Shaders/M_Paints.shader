				float _ALBEDOcontrast_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_ALBEDOcontrast);
				float4 temp_cast_1 = (_ALBEDOcontrast_Instance).xxxx;
				float _ALBEDOsaturation_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_ALBEDOsaturation);
				float3 desaturateInitialColor13 = pow( saferPower11 , temp_cast_1 ).rgb;
				float desaturateDot13 = dot( desaturateInitialColor13, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar13 = lerp( desaturateInitialColor13, desaturateDot13.xxx, ( 1.0 - _ALBEDOsaturation_Instance ) );
				
				float _NORMALpower_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_NORMALpower);
				float4 _NORMAL_ST_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_NORMAL_ST);
				float2 uv_NORMAL = IN.ase_texcoord7.xy * _NORMAL_ST_Instance.xy + _NORMAL_ST_Instance.zw;
				float3 normalizeResult39 = normalize( UnpackNormalScale( tex2D( _NORMAL, uv_NORMAL ), _NORMALpower_Instance ) );
				
				float _SPECULLAR_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_SPECULLAR);
				
				float _MASKcontrast_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_MASKcontrast);
				float4 _MASK_ST_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_MASK_ST);
				float2 uv_MASK = IN.ase_texcoord7.xy * _MASK_ST_Instance.xy + _MASK_ST_Instance.zw;
				float _MASKgain_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_MASKgain);
				float3 desaturateInitialColor34 = CalculateContrast(_MASKcontrast_Instance,( tex2D( _MASK, uv_MASK ) * _MASKgain_Instance )).rgb;
				float desaturateDot34 = dot( desaturateInitialColor34, float3( 0.299, 0.587, 0.114 ));
				float3 desaturateVar34 = lerp( desaturateInitialColor34, desaturateDot34.xxx, 0.0 );
				float _GLOSSINESScontrast_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_GLOSSINESScontrast);
				float _GLOSSINESSgain_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_GLOSSINESSgain);
				float4 blendOpSrc40 = float4( desaturateVar34 , 0.0 );
				float4 blendOpDest40 = CalculateContrast(_GLOSSINESScontrast_Instance,( tex2D( _GLOSSINESS, uv08 ) * _GLOSSINESSgain_Instance ));
				float _GLOSSMASKgain_Instance = UNITY_ACCESS_INSTANCED_PROP(M_Paints,_GLOSSMASKgain);
				float4 lerpBlendMode40 = lerp(blendOpDest40,( blendOpSrc40 * blendOpDest40 ),_GLOSSMASKgain_Instance);
				
				float3 Albedo = desaturateVar13;
				float3 Normal = normalizeResult39;
				float3 Emission = 0;
				float3 Specular = 0.5;
				float Metallic = _SPECULLAR_Instance;
				float Smoothness = ( saturate( lerpBlendMode40 )).r;
				float Occlusion = 1;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = 1;
				float3 Translucency = 1;

				#ifdef _ALPHATEST_ON
					clip(Alpha - AlphaClipThreshold);
				#endif

				InputData inputData;
				inputData.positionWS = WorldPosition;
				inputData.viewDirectionWS = WorldViewDirection;
				inputData.shadowCoord = ShadowCoords;

				#ifdef _NORMALMAP
					inputData.normalWS = normalize(TransformTangentToWorld(Normal, half3x3( WorldTangent, WorldBiTangent, WorldNormal )));
